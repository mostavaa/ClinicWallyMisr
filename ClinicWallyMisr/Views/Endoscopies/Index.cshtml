@model IEnumerable<ClinicWallyMisr.Endoscopy>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create", new { id = ViewBag.visit.id })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Endoscopy1)

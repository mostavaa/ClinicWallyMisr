            @Html.DisplayFor(modelItem => item.Endoscopy1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndoscopyType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.site)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.comments)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.graph)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.visit.visitStatus)
        </td>

@model ClinicWallyMisr.prescription

@{
    ViewBag.Title = "Delete";
}

<div style="margin-top:20px;clear:both;"></div>
<div class="row">
    <div class="col-md-10 col-md-offset-1">

        <h3>Are you sure you want to delete this?</h3>
        <div>
            <hr />
            <dl class="dl-horizontal row">
                <div class="row">
                    <div class="col-md-4">
                        <dt>
                            @Html.DisplayNameFor(model => model.notes)
                        </dt>

                        <dd class="myinput">
                            @Html.DisplayFor(model => model.notes)
                        </dd>
                    </div>
                    <div class="col-md-4">
                        <dt>
                            @Html.DisplayNameFor(model => model.visit.visitStatus)
                        </dt>

                        <dd class="myinput">
                            @Html.DisplayFor(model => model.visit.visitStatus)
                        </dd>
                    </div>
                    <div class="col-md-4">

                    </div>
                </div>

  

            </dl>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-actions no-color">
                    <input type="submit" value="Delete" class="btn btn-danger" /> |
                    @Html.ActionLink("Back to List", "Index", new { id = Model.visitId }, new { @class="btn btn-default"})
                </div>
            }
        </div>
        </div>
    </div>

<br />